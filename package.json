    "form-data": "^4.0.0",
    "got": "^11.8.6",
    "graphql": "^16.7.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^9.0.0",
    "graphql-ws": "^5.14.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
